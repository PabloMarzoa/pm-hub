{
	"name": "pmds-libs-plugin",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "plugins/pmds-libs-plugin/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/plugins/pmds-libs-plugin",
				"main": "plugins/pmds-libs-plugin/src/index.ts",
				"tsConfig": "plugins/pmds-libs-plugin/tsconfig.lib.json",
				"assets": [
					"plugins/pmds-libs-plugin/*.md",
					{
						"input": "./plugins/pmds-libs-plugin/src",
						"glob": "**/!(*.ts)",
						"output": "./src"
					},
					{
						"input": "./plugins/pmds-libs-plugin/src",
						"glob": "**/*.d.ts",
						"output": "./src"
					},
					{
						"input": "./plugins/pmds-libs-plugin",
						"glob": "generators.json",
						"output": "."
					},
					{
						"input": "./plugins/pmds-libs-plugin",
						"glob": "executors.json",
						"output": "."
					}
				]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "plugins/pmds-libs-plugin/jest.config.ts"
			}
		}
	},
	"tags": []
}
