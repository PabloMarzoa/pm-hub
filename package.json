		"@nx/jest": "21.2.0",
		"@nx/js": "21.2.0",
		"@nx/plugin": "21.2.0",
		"@nx/storybook": "21.2.0",
		"@nx/web": "21.2.0",
		"@nx/workspace": "21.2.0",
		"@schematics/angular": "~20.0.3",
		"@storybook/addon-essentials": "8.6.14",
