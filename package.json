{
	"name": "pmds-hub",
	"version": "20.0.1",
	"license": "MIT",
	"scripts": {
		"start": "nx serve pmds-hub-storybook",
		"build": "nx build --project=pmds-hub-storybook",
		"build:libs": "NODE_TLS_REJECT_UNAUTHORIZED=1 nx reset && nx run-many -t build -p pm-fuel",
		"build:storybook": " nx build-storybook pmds-hub-storybook",
		"storybook": "NODE_TLS_REJECT_UNAUTHORIZED=1 && nx storybook pmds-hub-storybook",
		"graph": "nx graph",
		"pmds:docu:build": "./pmds-docu-build.sh",
		"update:deps": "tsx ./scripts/update-dependencies.ts",
		"update:deps:dry": "tsx ./scripts/update-dependencies.ts --dry-run",
		"deps:graph": "tsx ./scripts/dependency-graph.ts",
		"deps:check": "tsx ./scripts/check-dependencies.ts",
		"publish:libs": "./scripts/publish-libs.sh",
		"publish:libs:dry": "./scripts/publish-libs.sh --dry-run"
	},
	"private": true,
	"workspaces": [
		"libs/*",
		"libs/pmds-cdk-libs/*",
		"libs/pmds-core-libs/*",
		"libs/pmds-directive-libs/*",
		"libs/pmds-pipe-libs/*",
		"libs/pmds-util-libs/*"
	],
	"dependencies": {
		"@angular/animations": "~20.0.4",
		"@angular/cdk": "^20.0.0",
		"@angular/common": "^20.0.0",
		"@angular/compiler": "~20.0.4",
		"@angular/core": "^20.0.0",
		"@angular/forms": "^20.0.0",
		"@angular/platform-browser": "~20.0.4",
		"@angular/platform-browser-dynamic": "~20.0.4",
		"@angular/router": "^20.0.0",
		"@nx/angular": "21.2.0",
		"dayjs": "^1.11.13",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rxjs": "~7.8.2",
		"tslib": "^2.8.1",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~20.0.3",
		"@angular-devkit/core": "~20.0.3",
		"@angular-devkit/schematics": "~20.0.3",
		"@angular-eslint/eslint-plugin": "~20.1.0",
		"@angular-eslint/eslint-plugin-template": "~20.1.0",
